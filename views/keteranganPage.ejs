<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Keterangan</title>
    <link href="/css/style.css" rel="stylesheet" type="text/css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <%- include('header') %>
  </head>
  <body>
    <header class="header">Summary</header>
    <div class="container-fluid py-3">
      <table class="table-bordered">
        <tbody>
          <tr>
            <th>ID</th>
            <% keteranganssObj.mereks.forEach((keterangan) => { %>
            <td><%= keterangan.id %></td>
            <% }) %>
          </tr>
          <tr>
            <th>Shoes Name</th>
            <% keteranganssObj.shoess.forEach((shoes) => { %>
            <td><%= shoes.name %></td>
            <% }) %>
          </tr>
          <tr>
            <th>Suplier Name</th>
            <% keteranganssObj.supliers.forEach((suplier) => { %>
            <td><%= suplier.name %></td>
            <% }) %>
          </tr>
          <tr>
            <th>Suplier Photo</th>
            <% keteranganssObj.supliers.forEach((suplier) => { %>
            <td><img class="card-img" src="<%= suplier.image %>" /></td>
            <% }) %>
          </tr>
          <tr>
            <th>Merek Qty</th>
            <% keteranganssObj.mereks.forEach((merek) => { %>
            <td><%= merek.qty %></td>
            <% }) %>
          </tr>
          <tr>
            <th>Gross Amount</th>
            <% keteranganssObj.mereks.forEach((merek) => { %>
            <td><%= merek.grossAmount %></td>
            <% }) %>
          </tr>
        </tbody>
      </table>
    </div>
  </body>
</html>
